{
  "compilerOptions": {
    "lib": [
      "DOM",
      "ES2021"
    ],
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "moduleResolution": "bundler",
    "useDefineForClassFields": true,
    "types": [
      "@rslib/core/types"
    ]
  },
  "include": [
    "src"
  ]
}